		{
			"name": "PUT /pet",
			"item": [
				{
					"name": "Update an existing pet",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "02676e84-0c85-4cfe-930a-6be0674eec15",
								"exec": [
									"pm.test('Status code is 200', function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test('Response time > 1000 ms', function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"\r",
									"// Visualization: Linear chart plotting pet id (x-axis) vs name length (y-axis)\r",
									"var template = `\r",
									"<canvas id=\"petChart\" height=\"75\"></canvas>\r",
									"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js\"></script>\r",
									"<script>\r",
									"    var ctx = document.getElementById(\"petChart\");\r",
									"    var petChart = new Chart(ctx, {\r",
									"        type: \"line\",\r",
									"        data: {\r",
									"            labels: [],\r",
									"            datasets: [{\r",
									"                label: 'Name Length',\r",
									"                data: [],\r",
									"                borderColor: 'rgba(75, 192, 192, 1)',\r",
									"                backgroundColor: 'rgba(75, 192, 192, 0.2)',\r",
									"                fill: true\r",
									"            }]\r",
									"        },\r",
									"        options: {\r",
									"            legend: { display: true },\r",
									"            title: {\r",
									"                display: true,\r",
									"                text: 'Pet ID vs Name Length'\r",
									"            },\r",
									"            scales: {\r",
									"                xAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: 'Pet ID'\r",
									"                    }\r",
									"                }],\r",
									"                yAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: 'Name Length'\r",
									"                    }\r",
									"                }]\r",
									"            }\r",
									"        }\r",
									"    });\r",
									"\r",
									"    pm.getData(function (err, value) {\r",
									"        petChart.data.labels = value.pets.map(function(pet) { return pet.id; });\r",
									"        petChart.data.datasets[0].data = value.pets.map(function(pet) { return pet.nameLength; });\r",
									"        petChart.update();\r",
									"    });\r",
									"</script>`;\r",
									"\r",
									"function constructVisualizerPayload() {\r",
									"    var response = pm.response.json();\r",
									"    var pets;\r",
									"    \r",
									"    // Handle both single pet object and array of pets\r",
									"    if (Array.isArray(response)) {\r",
									"        pets = response;\r",
									"    } else {\r",
									"        pets = [response];\r",
									"    }\r",
									"    \r",
									"    return {\r",
									"        pets: pets.map(function(pet) {\r",
									"            return {\r",
									"                id: pet.id,\r",
									"                nameLength: pet.name ? pet.name.length : 0\r",
									"            };\r",
									"        })\r",
									"    };\r",
									"}\r",
									"\r",
									"pm.visualizer.set(template, constructVisualizerPayload());"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
