								"exec": [
									"pm.test('Status code is 200', function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test('Body has property Status', function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.have.property('status');\r",
									"});\r",
									"\r",
									"pm.test('Body has property Category', function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.have.property('category');\r",
									"});\r",
									"\r",
									"pm.environment.set(\"petId\", pm.response.json().id);\r",
									"\r",
									"// Visualization script\r",
									"var template = `\r",
									"<canvas id=\"barChart\" height=\"100\"></canvas>\r",
									"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js\"></script>\r",
									"<script>\r",
									"    var ctx = document.getElementById(\"barChart\");\r",
									"    var barChart = new Chart(ctx, {\r",
									"        type: \"bar\",\r",
									"        data: {\r",
									"            labels: [],\r",
									"            datasets: [{\r",
									"                label: \"Numeric Values\",\r",
									"                backgroundColor: \"rgba(54, 162, 235, 0.6)\",\r",
									"                borderColor: \"rgba(54, 162, 235, 1)\",\r",
									"                borderWidth: 1,\r",
									"                data: []\r",
									"            }]\r",
									"        },\r",
									"        options: {\r",
									"            legend: { display: true },\r",
									"            title: {\r",
									"                display: true,\r",
									"                text: \"Response Numeric Properties & Array Lengths\"\r",
									"            },\r",
									"            scales: {\r",
									"                xAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: \"Property\"\r",
									"                    }\r",
									"                }],\r",
									"                yAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: \"Value\"\r",
									"                    },\r",
									"                    ticks: {\r",
									"                        beginAtZero: true\r",
									"                    }\r",
									"                }]\r",
									"            }\r",
									"        }\r",
									"    });\r",
									"\r",
									"    pm.getData(function (err, value) {\r",
									"        barChart.data.labels = value.labels;\r",
									"        barChart.data.datasets[0].data = value.values;\r",
									"        barChart.update();\r",
									"    });\r",
									"</script>\r",
									"`;\r",
									"\r",
									"function constructVisualizerPayload() {\r",
									"    var jsonData = pm.response.json();\r",
									"    var labels = [];\r",
									"    var values = [];\r",
									"    \r",
									"    for (var key in jsonData) {\r",
									"        if (jsonData.hasOwnProperty(key)) {\r",
									"            var value = jsonData[key];\r",
									"            if (typeof value === 'number') {\r",
									"                labels.push(key);\r",
									"                values.push(value);\r",
									"            } else if (Array.isArray(value)) {\r",
									"                labels.push(key + \" (length)\");\r",
									"                values.push(value.length);\r",
									"            }\r",
									"        }\r",
									"    }\r",
									"    \r",
									"    return {\r",
									"        labels: labels,\r",
									"        values: values\r",
									"    };\r",
									"}\r",
									"\r",
									"pm.visualizer.set(template, constructVisualizerPayload());\r",
									""
								],
