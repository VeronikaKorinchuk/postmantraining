		{
			"name": "GET /pet/findByStatus",
			"item": [
				{
					"name": "Find pet by status",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"id": "3e3135d9-ee75-494c-a5ae-bfb099599f86",
								"exec": [
									"const values = [\"sold\", \"pending\", \"available\"];\r",
									"const randomValue = values[Math.floor(Math.random() * values.length)];\r",
									"pm.collectionVariables.set(\"status\", randomValue);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"id": "7e4a520f-99e6-4ef6-ab34-ee346bddb205",
								"exec": [
									"pm.test('Status code is 200', function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test('All pets have expected status', function () {\r",
									"    const expectedStatus = pm.variables.get('status');\r",
									"    const body = pm.response.json();\r",
									"\r",
									"    body.forEach(pet => {\r",
									"        pm.expect(pet.status).to.equal(expectedStatus);\r",
									"    });\r",
									"});\r",
									"\r",
									"\r",
									"\r",
									"// Visualization\r",
									"var template = `\r",
									"<style>\r",
									"    .container { font-family: Arial, sans-serif; padding: 20px; }\r",
									"    .search-box { \r",
									"        width: 100%; \r",
									"        padding: 10px; \r",
									"        margin-bottom: 15px; \r",
									"        border: 1px solid #ddd; \r",
									"        border-radius: 4px; \r",
									"        font-size: 14px;\r",
									"    }\r",
									"    table { \r",
									"        width: 100%; \r",
									"        border-collapse: collapse; \r",
									"        margin-top: 10px;\r",
									"    }\r",
									"    th, td { \r",
									"        border: 1px solid #ddd; \r",
									"        padding: 10px; \r",
									"        text-align: left; \r",
									"    }\r",
									"    th { \r",
									"        background-color: #f4f4f4; \r",
									"        font-weight: bold; \r",
									"    }\r",
									"    tr:nth-child(even) { background-color: #f9f9f9; }\r",
									"    tr:hover { background-color: #f1f1f1; }\r",
									"    .no-data { color: #999; font-style: italic; }\r",
									"</style>\r",
									"<div class=\"container\">\r",
									"    <input type=\"text\" id=\"searchInput\" class=\"search-box\" placeholder=\"Search pets...\" onkeyup=\"filterTable()\">\r",
									"    <table id=\"petTable\">\r",
									"        <thead>\r",
									"            <tr>\r",
									"                <th>ID</th>\r",
									"                <th>Name</th>\r",
									"                <th>Status</th>\r",
									"                <th>Category</th>\r",
									"                <th>Tags</th>\r",
									"                <th>Photo URL</th>\r",
									"            </tr>\r",
									"        </thead>\r",
									"        <tbody>\r",
									"            {{#if pets.length}}\r",
									"                {{#each pets}}\r",
									"                <tr>\r",
									"                    <td>{{id}}</td>\r",
									"                    <td>{{name}}</td>\r",
									"                    <td>{{status}}</td>\r",
									"                    <td>{{category}}</td>\r",
									"                    <td>{{tags}}</td>\r",
									"                    <td>{{photoUrl}}</td>\r",
									"                </tr>\r",
									"                {{/each}}\r",
									"            {{else}}\r",
									"                <tr><td colspan=\"6\" class=\"no-data\">No pets found</td></tr>\r",
									"            {{/if}}\r",
									"        </tbody>\r",
									"    </table>\r",
									"</div>\r",
									"<script>\r",
									"    function filterTable() {\r",
									"        var input = document.getElementById(\"searchInput\");\r",
									"        var filter = input.value.toLowerCase();\r",
									"        var table = document.getElementById(\"petTable\");\r",
									"        var rows = table.getElementsByTagName(\"tr\");\r",
									"        \r",
									"        for (var i = 1; i < rows.length; i++) {\r",
									"            var cells = rows[i].getElementsByTagName(\"td\");\r",
									"            var match = false;\r",
									"            for (var j = 0; j < cells.length; j++) {\r",
									"                if (cells[j] && cells[j].textContent.toLowerCase().indexOf(filter) > -1) {\r",
									"                    match = true;\r",
									"                    break;\r",
									"                }\r",
									"            }\r",
									"            rows[i].style.display = match ? \"\" : \"none\";\r",
									"        }\r",
									"    }\r",
									"</script>\r",
									"`;\r",
									"\r",
									"function constructVisualizerPayload() {\r",
									"    var response = pm.response.json();\r",
									"    var pets = [];\r",
									"    \r",
									"    if (Array.isArray(response)) {\r",
									"        pets = response.map(function(pet) {\r",
									"            var categoryName = (pet.category && pet.category.name) ? pet.category.name : '-';\r",
									"            \r",
									"            var tagsStr = '-';\r",
									"            if (pet.tags && Array.isArray(pet.tags) && pet.tags.length > 0) {\r",
									"                var tagNames = pet.tags.map(function(tag) {\r",
									"                    return tag.name || '';\r",
									"                }).filter(function(name) {\r",
									"                    return name !== '';\r",
									"                });\r",
									"                tagsStr = tagNames.length > 0 ? tagNames.join(', ') : '-';\r",
									"            }\r",
									"            \r",
									"            var photoUrl = '-';\r",
									"            if (pet.photoUrls && Array.isArray(pet.photoUrls) && pet.photoUrls.length > 0 && pet.photoUrls[0]) {\r",
									"                photoUrl = pet.photoUrls[0];\r",
									"            }\r",
									"            \r",
									"            return {\r",
									"                id: pet.id !== undefined ? pet.id : '-',\r",
									"                name: pet.name || '-',\r",
									"                status: pet.status || '-',\r",
									"                category: categoryName,\r",
									"                tags: tagsStr,\r",
									"                photoUrl: photoUrl\r",
									"            };\r",
									"        });\r",
									"    }\r",
									"    \r",
									"    return { pets: pets };\r",
									"}\r",
									"\r",
									"pm.visualizer.set(template, constructVisualizerPayload());\r",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
